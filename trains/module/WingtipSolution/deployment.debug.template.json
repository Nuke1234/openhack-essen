{
  "$schema-template": "1.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "docker": {
                "username": "$CONTAINER_REGISTRY_USERNAME_docker",
                "password": "$CONTAINER_REGISTRY_PASSWORD_docker",
                "address": "wingtipcontainerreg.azurecr.io"
              }
            }
          }
        },
        "systemModules": {
          "edgeAgent": {
            "type": "docker",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-agent:1.0",
              "createOptions": {}
            }
          },
          "edgeHub": {
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "mcr.microsoft.com/azureiotedge-hub:1.0",
              "createOptions": {
                "HostConfig": {
                  "PortBindings": {
                    "5671/tcp": [
                      {
                        "HostPort": "5671"
                      }
                    ],
                    "8883/tcp": [
                      {
                        "HostPort": "8883"
                      }
                    ],
                    "443/tcp": [
                      {
                        "HostPort": "443"
                      }
                    ]
                  }
                }
              }
            }
          }
        },
        "modules": {
          "Events": {
            "type": "docker",
            "settings": {
                "image": "openhack/iot-sim-3:3.0-stretch",
                "createOptions": ""
            },
            "status": "running",
            "restartPolicy": "always",
            "version": "1.0"
          },
          "GPSData": {
            "type": "docker",
            "settings": {
                "image": "openhack/iot-sim-1:3.0-stretch",
                "createOptions": ""
            },
            "status": "running",
            "restartPolicy": "always",
            "version": "1.0"
          },
          "Accelerometer": {
            "type": "docker",
            "settings": {
                "image": "openhack/iot-sim-2:3.0-stretch",
                "createOptions": ""
            },
            "status": "running",
            "restartPolicy": "always",
            "version": "1.0"
          },
          "PhotoTriggeringCheckModule": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "settings": {
              "image": "${MODULES.PhotoTriggeringCheckModule.debug}",
              "createOptions": {
                "ExposedPorts": {
                  "9229/tcp": {}
                },
                "HostConfig": {
                  "PortBindings": {
                    "9229/tcp": [
                      {
                        "HostPort": "9229"
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.0",
        "routes": {
          "PhotoTriggeringCheckModuleToIoTHub": "FROM /messages/modules/PhotoTriggeringCheckModule/outputs/* INTO $upstream",
          "sensorToPhotoTriggeringCheckModule": "FROM /messages/modules/Events/outputs/train INTO BrokeredEndpoint(\"/modules/PhotoTriggeringCheckModule/inputs/input1\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}